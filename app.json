{
  "expo": {
    "name": "choreplan",
    "slug": "choreplan",
    "version": "1.0.1",
    "icon": "./assets/icon_app.png",   
    "orientation": "portrait", 
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.feriteja.choreplan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon_app.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1228372b-002d-4bd0-8dc6-7f48a2c97cdc"
      }
    },
    "owner": "feriteja"
  }
}
